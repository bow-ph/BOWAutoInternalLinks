{% block bow_log_viewer %}
    <div class="bow-log-viewer">
        {% block bow_log_viewer_content %}
            <sw-card-view>
                {% block bow_log_viewer_content_card %}
                    <sw-card :isLoading="isLoading">
                        {% block bow_log_viewer_content_card_grid %}
                            <sw-data-grid
                                :dataSource="logStats"
                                :columns="columns"
                                :showSettings="true"
                                :showSelection="false">
                            </sw-data-grid>
                        {% endblock %}
                    </sw-card>
                {% endblock %}
            </sw-card-view>
        {% endblock %}
    </div>
{% endblock %}
