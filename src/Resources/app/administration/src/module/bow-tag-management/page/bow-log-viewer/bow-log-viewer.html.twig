{% block bow_log_viewer %}
    <sw-page class="bow-log-viewer">
        {% block bow_log_viewer_smart_bar %}
            <template slot="smart-bar-header">
                <h2>{{ $tc('bow-tag-management.general.statistics') }}</h2>
            </template>
        {% endblock %}

        {% block bow_log_viewer_content %}
            <sw-card-view slot="content">
                {% block bow_log_viewer_content_card %}
                    <sw-card :isLoading="isLoading">
                        {% block bow_log_viewer_content_card_grid %}
                            <sw-data-grid
                                :dataSource="logStats"
                                :columns="columns"
                                :showSettings="true"
                                :showSelection="false">
                            </sw-data-grid>
                        {% endblock %}
                    </sw-card>
                {% endblock %}
            </sw-card-view>
        {% endblock %}
    </sw-page>
{% endblock %}
