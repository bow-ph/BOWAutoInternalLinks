{% block bow_log_viewer %}
    <sw-card-view class="bow-log-viewer">
        {% block bow_log_viewer_content %}
            <sw-card 
                :isLoading="isLoading"
                :large="true">
                {% block bow_log_viewer_content_grid %}
                    <sw-data-grid
                        :dataSource="logStats"
                        :columns="columns"
                        :showSettings="true"
                        :showSelection="false">
                    </sw-data-grid>
                {% endblock %}
            </sw-card>
        {% endblock %}
    </sw-card-view>
{% endblock %}
